
{
  "presets": [
    [
      "next/babel",
      {
        "preset-env":  {
          "useBuiltIns": "usage",
          "corejs": 2,
          "targets": {
            "ie": 11
          }
        }
      }
    ]
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    ["lodash"],
    ["@babel/plugin-transform-runtime"]
  ]
}